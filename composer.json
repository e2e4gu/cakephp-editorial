{
    "name": "mindforce/cakephp-editorial",
    "description": "Editorial Core plugin for CakePHP",
	"homepage": "http://mindforce.me",
	"keywords": ["cakephp", "ui", "editor"],
	"type": "cakephp-plugin",
	"license": "MIT",
	"authors": [
		{
			"name": "Denys Kyselov",
			"homepage": "https://mindforce.me"
		}
	],
	"support": {
		"issues": "https://github.com/mindforce/cakephp-editorial/issues",
		"source": "https://github.com/mindforce/cakephp-editorial"
	},
	"minimum-stability": "dev",
    "require": {
        "php": ">=5.6",
        "cakephp/cakephp": "^3.7",
        "cakephp/plugin-installer": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Editorial\\Core\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Editorial\\Core\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
